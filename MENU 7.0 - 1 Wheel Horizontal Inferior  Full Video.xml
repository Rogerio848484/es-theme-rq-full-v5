<theme>
   <formatVersion>7</formatVersion>

<view name="system">

<text name="systemInfo">
    <glowColor>000000</glowColor>
    <pos>0 0.915</pos>
    <backgroundColor>00000000</backgroundColor>
    <color>ffffff</color>
    <rotation>0</rotation> 
    <fontPath>./_inc/fonts/gomarice_no_continue.ttf</fontPath>
     <fontSize>0.040</fontSize>
     <zIndex>5001</zIndex>	     
</text> 

<video name="backgroundvideo7" extra="true">
<visible>true</visible>
<path>{random}</path> 
<rotation>0</rotation>  
<pos>0.500 0.501</pos>
<origin>0.5 0.5</origin>
<delay>2</delay>
<efect>true</efect>
<showSnapshotNoVideo>true</showSnapshotNoVideo>
<showSnapshotDelay>true</showSnapshotDelay>
<color>FFFFFF</color>
<zIndex>5000</zIndex>
</video>

