<theme>
   <formatVersion>7</formatVersion>

<view name="system">

<text name="systemInfo">
    <glowColor>000000</glowColor>
    <pos>0 0.6</pos>
    <backgroundColor>00000000</backgroundColor>
    <color>ffffff</color>
    <rotation>0</rotation> 
    <fontPath>./_inc/fonts/gomarice_no_continue.ttf</fontPath>
     <fontSize>0.040</fontSize>
     <zIndex>999</zIndex>	     
</text> 

<image name="wallpaperheder" extra="true">
<visible>false</visible>
</image>

<image name="wallpaper" extra="true">
<visible>false</visible>
</image>

<image name="wallpaper1" extra="true">
<pos>0 0</pos>
<size>1 1</size>
<path>./default/_inc/wall.jpg</path>
<path>./_inc/wallpaper4/${system.theme}.jpg</path>
<zIndex>1</zIndex>
</image>

  
<helpsystem name="help">
<fontSize>0.034</fontSize>
<pos>0 0.965</pos>    
<glowColor>000000</glowColor>
<backgroundColor>000000</backgroundColor>
<color>ffffff</color>	  
<textColor>ffffff</textColor>
<iconColor>ffffff</iconColor>
</helpsystem>
	  
<carousel name="systemcarousel">
<type>gamecarousel</type>
<pos>0 0.399999</pos>
<size>1 0.2</size>
<zIndex>5000</zIndex>   
<backgroundColor>000000ff</backgroundColor>		
<logoSize>0.30 0.12</logoSize>
<logoAlignment>center</logoAlignment>
<maxLogoCount>3</maxLogoCount>
<zIndex>1000</zIndex>
</carousel>
	  
<image name="logo">
<path>./default/_inc/logo.png</path>
<path>./_inc/logosclean/${system.theme}.png</path>	
</image>
	  
